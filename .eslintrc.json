{
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "plugins": [
        "import"
    ],
    "globals": {
      "ActiveXObject": true
    },
    "rules": {
      "no-console": 0,
      "no-restricted-syntax": 0,
      "no-prototype-builtins": 0,
      "no-continue": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "valid-typeof": ["error", {
        "requireStringLiterals": false
        }
      ],
      "prefer-const": 0,
      "func-names": 0,
      "no-plusplus": 0,
      "no-mixed-operators": 0,
      "class-methods-use-this": 0,
      "no-use-before-define": 0
    }
}
